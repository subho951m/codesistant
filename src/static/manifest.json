{
  "manifest_version": 3,
  "name": "Codesistant",
  "description": "Suggest coding problems everyday, week's unsolved problems and favourite problems.",
  "version": "1.0.2",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "action": {
    "default_title": "Codesistant",
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["storage"],
  "host_permissions": ["https://codeforces.com/*"],
  "options_page": "options.html"
}
